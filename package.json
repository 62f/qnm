{
  "name": "qnm",
  "version": "0.13.1",
  "description": "cli utility for querying the node_modules directory",
  "bin": "bin/qnm.js",
  "repository": "ranyitz/qnm",
  "engines": {
    "node": ">=8.7"
  },
  "files": [
    "bin",
    "src",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "query",
    "node_modules",
    "version",
    "cli"
  ],
  "author": "Ran Yitzhaki",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "test:watch": "jest --watch",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{md,json}": [
      "prettier"
    ]
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^23.0.0",
    "lint-staged": "^7.1.2",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "chalk": "^2.4.1",
    "commander": "^2.15.0",
    "figures": "^2.0.0",
    "fuzzysort": "^1.1.1",
    "js-levenshtein": "^1.1.3",
    "lodash": "^4.17.5",
    "log-update": "^2.3.0",
    "opn": "^5.3.0",
    "pkg-dir": "^2.0.0",
    "prompts": "^0.1.8",
    "tabtab": "^2.2.2",
    "update-notifier": "^2.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.spec.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
    ],
    "watchPathIgnorePatterns": [
      "node_modules"
    ],
    "setupFiles": [
      "./__tests__/setup.js"
    ]
  }
}
